{
    "name": "home-marketing",
    "version": "1.0.0",
    "description": "SmartZip-style real estate lead generation marketing site",
    "main": "index.js",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:report": "playwright show-report",
        "test:e2e:install": "playwright install",
        "redfin:build-cache": "node scripts/build-redfin-cache.mjs",
        "census:build-cache": "tsx scripts/build-census-cache.ts",
        "fred:build-cache": "tsx scripts/build-fred-cache.ts",
        "hud:build-cache": "tsx scripts/build-hud-cache.ts",
        "market:refresh": "tsx scripts/dev/refresh-market-data.ts",
        "market:validate": "tsx scripts/dev/validate-market-data.ts",
        "feature-store:refresh": "tsx scripts/jobs/refresh-seller-feature-store.ts",
        "nightly:seller": "tsx scripts/jobs/nightly-seller-refresh.ts",
        "health:seller": "tsx scripts/jobs/check-seller-health.ts",
        "model:train": "tsx scripts/dev/train-seller-model.ts",
        "model:backtest": "tsx scripts/dev/backtest-seller-model.ts",
        "build-all-caches": "npm run market:refresh && npm run feature-store:refresh",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "dev:start": "powershell -ExecutionPolicy Bypass -File scripts/dev/start-dev.ps1",
        "dev:stop": "powershell -ExecutionPolicy Bypass -File scripts/dev/stop-dev.ps1"
    },
    "keywords": [
        "real-estate",
        "marketing",
        "lead-generation",
        "nextjs",
        "react"
    ],
    "author": "seelander09",
    "license": "MIT",
    "dependencies": {
        "@hookform/resolvers": "^3.3.4",
        "@sanity/client": "^6.15.11",
        "@sanity/image-url": "^1.0.2",
        "chart.js": "^4.4.6",
        "clsx": "^1.2.1",
        "embla-carousel-react": "^8.1.6",
        "framer-motion": "^10.16.0",
        "groq": "^3.46.2",
        "leaflet": "^1.9.4",
        "next": "^14.2.7",
        "next-sanity": "^8.5.5",
        "react": "^18.3.1",
        "react-chartjs-2": "^5.2.0",
        "react-countup": "^6.5.3",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "react-leaflet": "^4.2.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@lhci/cli": "^0.14.0",
        "@playwright/test": "^1.56.0",
        "@sanity/cli": "^3.7.1",
        "@sanity/vision": "^3.7.1",
        "@storybook/addon-essentials": "^8.2.5",
        "@storybook/addon-interactions": "^8.2.5",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/blocks": "^8.2.5",
        "@storybook/react": "^8.2.5",
        "@storybook/react-webpack5": "^8.6.14",
        "@storybook/test": "^8.2.5",
        "@tailwindcss/forms": "^0.5.7",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^14.2.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/leaflet": "^1.9.20",
        "@types/node": "^20.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "autoprefixer": "^10.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^8.0.0",
        "eslint-config-next": "^14.0.0",
        "postcss": "^8.0.0",
        "postcss-loader": "^8.2.0",
        "prettier": "^3.3.3",
        "sanity": "^3.7.1",
        "storybook": "^8.2.5",
        "style-loader": "^4.0.0",
        "tailwindcss": "^3.4.9",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.6",
        "typescript": "^5.5.4",
        "vitest": "^1.6.0"
    }
}
